{
  "name": "ota-backend",
  "version": "1.0.0",
  "description": "OTA Backend API with MySQL and Prisma",
  "main": "server.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "db:migrate": "prisma migrate dev",
    "db:migrate:seed": "prisma migrate dev --seed",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:refresh": "prisma migrate reset --force && node prisma/seed.js",
    "db:studio": "prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"No linter configured\"",
    "build": "echo \"No build step required for Node.js\""
  },
  "keywords": [
    "api",
    "nodejs",
    "express",
    "mysql",
    "prisma"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.9.4"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.0",
    "prisma": "^6.17.1"
  }
}
